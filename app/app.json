{
  "expo": {
    "name": "TrobotScout",
    "slug": "tigerscouting",
    "privacy": "public",
    "description": "An easy, open source cloud-integrated scouting app made by 5013.",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E1E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/497c6dc6-9bab-4f75-a754-ee8bf537238c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.team5013.trobotscout",
      "buildNumber":"5",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow TrobotScout to access your camera. TrobotScout uses the camera exclusively to scan QR codes and allows you to connect to cloud storage buckets."
      }
    },
    "android": {
      "package": "com.team5013.trobotscout",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1E1E1E"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "497c6dc6-9bab-4f75-a754-ee8bf537238c"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow TrobotScout to access your camera. TrobotScout uses the camera exclusively to scan QR codes and allows you to connect to cloud storage buckets."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "sdkVersion": "49.0.0"
  },
  "name": "trobotscout2.0"
}
